<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bbahealth">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="PRCO203"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Radiotherapy">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".InfoOpenActivity" />
        <activity
            android:name=".InitialActivity"
            android:label="@string/title_activity_initial"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ZoomTwoActivity" />
        <activity android:name=".ZoomOneActivity" />
        <activity android:name=".TrackerInfoOpen"></activity>
    </application>

</manifest>